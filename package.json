{
  "name": "sourcemapify",
  "version": "1.1.4",
  "description": "Source Map plugin for Browserify",
  "keywords": [
    "browserify",
    "browserify-plugin",
    "sourcemap",
    "source-map",
    "source map"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://bigstickcarpet.com"
  },
  "homepage": "https://github.com/BigstickCarpet/sourcemapify",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/sourcemapify.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib test/fixtures test/specs --fix",
    "test": "mocha && npm run lint",
    "cover": "istanbul cover _mocha",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm run test && npm run bump && npm publish"
  },
  "devDependencies": {
    "browserify": "^16.1.1",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.3",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-modular": "^4.1.0",
    "exorcist": "^1.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.5",
    "npm-check": "^5.5.2",
    "npm-check-updates": "^2.13.0",
    "version-bump-prompt": "^4.0.0"
  },
  "dependencies": {
    "through": "^2.3.8"
  },
  "engines": {
    "node": ">=4"
  }
}