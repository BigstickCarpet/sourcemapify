{
  "name": "sourcemapify",
  "version": "1.1.6",
  "description": "Source Map plugin for Browserify",
  "keywords": [
    "browserify",
    "browserify-plugin",
    "sourcemap",
    "source-map",
    "source map"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "homepage": "https://jsdevtools.org/sourcemapify",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/sourcemapify.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib test/fixtures test/specs --fix",
    "test": "mocha && npm run lint",
    "coverage": "nyc --reporter=text --reporter=lcov mocha",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm run test && npm run bump && npm publish"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.1.0",
    "coveralls": "^3.0.2",
    "del": "^3.0.0",
    "eslint": "^5.6.1",
    "eslint-config-modular": "^4.2.2",
    "exorcist": "^1.0.0",
    "mocha": "^5.2.0",
    "npm-check": "^5.9.0",
    "npm-check-updates": "^2.14.2",
    "nyc": "^13.0.1",
    "version-bump-prompt": "^4.2.1"
  },
  "dependencies": {
    "through": "^2.3.8"
  },
  "engines": {
    "node": ">=4"
  }
}
