{
  "name": "sourcemapify",
  "version": "1.0.4",
  "description": "Source Map plugin for Browserify",
  "keywords": [
    "browserify",
    "browserify-plugin",
    "sourcemap",
    "source-map",
    "source map"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/BigstickCarpet/sourcemapify",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib test --fix",
    "test": "mocha",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm run lint && npm run test && npm run bump && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/sourcemapify.git"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.9.0",
    "eslint-config-modular": "^1.0.1",
    "exorcist": "^0.4.0",
    "mocha": "^3.1.2",
    "npm-check": "^5.4.0",
    "npm-check-updates": "^2.2.0",
    "version-bump-prompt": "^1.5.0"
  },
  "dependencies": {
    "through": "^2.3.8"
  }
}
